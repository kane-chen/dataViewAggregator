package cn.kane.web.view.aggregator.script;

import org.apache.commons.logging.Log;
import org.apache.commons.logging.LogFactory
import cn.kane.web.view.aggregator.util.groovy.annotation.ContextBean;
import cn.kane.web.view.aggregator.service.inner.DataReadService ;


public class SimpleDataReadService implements DataReadService {

    private static final Log LOGGER = LogFactory.getLog(SimpleDataReadService.class);
    @ContextBean(beanName="simpleDataReadService")
    private DataReadService simpleDataReadService ;
    
    @Override
    public Object getData(Object param) {
        LOGGER.info(String.format("simple.getData %s", param));
        try {
        	return simpleDataReadService.getData(param);
        } catch (Exception e) {
            LOGGER.error(String.format("simple.getData %s", param), e);
        }
        return null;
    }

}